{
  "kind": "collectionType",
  "collectionName": "single_posts",
  "info": {
    "singularName": "single-post",
    "pluralName": "single-posts",
    "displayName": "Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true
    },
    "TituloPost": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "ConteudoPost": {
      "type": "blocks"
    },
    "BannerPost": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "TitleSEO": {
      "type": "string",
      "unique": true
    },
    "DescriptionSEO": {
      "type": "text"
    },
    "OGImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "categorias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::categoria.categoria",
      "inversedBy": "posts"
    },
    "fixarTopo": {
      "type": "boolean",
      "default": false
    }
  }
}
